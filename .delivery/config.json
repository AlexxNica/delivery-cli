{
  "version": "2",
  "build_cookbook": {
    "name": "delivery_rust",
    "path": "cookbooks/delivery_rust"
  },
  "job_dispatch": {
    "version": "v2",
    "filters": {
      "unit": [
        {
          "platform_family": ["debian"],
          "platform_version": ["12.04"]
        },
        {
          "platform_family": ["debian"],
          "platform_version": ["14.04"]
        },
        {
          "platform_family": ["rhel"]
        }
      ],
      "syntax": [
        {
          "platform_family": ["debian"],
          "platform_version": ["12.04"]
        },
        {
          "platform_family": ["debian"],
          "platform_version": ["14.04"]
        },
        {
          "platform_family": ["rhel"]
        }
      ]
    }
  },
  "skip_phases": [
    "lint",
    "security",
    "smoke",
    "deploy",
    "publish",
    "quality",
    "provision"
  ]
}
