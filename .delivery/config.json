{
    "version": "2",
    "build_cookbook": {
        "name": "delivery_rust",
        "path": "cookbooks/delivery_rust"
    },
    "skip_phases": [
      "lint",
      "quality",
      "security",
      "smoke",
      "deploy"
    ],
    "build_nodes": {
        "default"    : ["name:builder*.shd.chef.co"],
        "unit"       : ["name:builder*.shd.chef.co AND platform_family:debian", "name:builder*.shd.chef.co AND platform_family:rhel"],
        "syntax"     : ["name:builder*.shd.chef.co AND platform_family:debian", "name:builder*.shd.chef.co AND platform_family:rhel"],
        "publish"    : ["name:builder*.shd.chef.co AND platform_family:debian", "name:builder*.shd.chef.co AND platform_family:rhel"]
    }
}
